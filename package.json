{
  "name": "slim-fetchy",
  "version": "1.0.1",
  "description": "fetch json-enhanced api (ES6) - using isomorphic-fetch lib",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "test": "babel-node test/test.js | faucet",
    "testes6": "node  --harmony --harmony_modules --harmony_sloppy_function --harmony_sloppy_let --harmony_default_parameters --es_staging test/test.js",
    "start": "webpack --config webpack.config.js && uglifyjs --compress --mangle --screw-ie8 -o ./dist/slim-fetchy.min.js -- ./dist/slim-fetchy.js"
  },
  "author": "Dan Levy <Dan@DanLevy.net>",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^3.2.1",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "faucet": "0.0.1",
    "tape": "^4.5.1",
    "webpack": "^1.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justsml/slim-fetchy"
  }
}
